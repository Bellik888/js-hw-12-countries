ate(data))\n        .catch(error => console.log(error));\n    \n}\n\nfunction updateTemplate(data) {\n    const markup = countryTemplate(data);\n    const markupList = countryList(data);\n\n    if (!data.length || data.length === \"\") {\n    info({\n      text: `You enter empty string `,\n    });\n    };\n\n    if (data.status === 404) {\n    error({\n      text: \"No country has been found. Please enter a more specific query!\",\n    });\n    }\n    if (data.length <= 10) {\n        refs.countryList.insertAdjacentHTML('beforeend', markupList);\n    }\n    if (data.length > 10) {\n    notice({\n      text: `Please enter a more specific query !`,\n    });\n    }\n\n    if (data.length === 1) {\n    refs.countryList.innerHTML = \"\";\n    refs.countryList.insertAdjacentHTML(\"beforeend\", markup);\n  }\n\n  \n}\n\nfunction clearInput() {\n    refs.countryList.innerHTML = \"\";\n}"},"lineCount":null}},"error":null,"hash":"6175a6b09454e4f5bfdd950ba9f4b793","cacheData":{"env":{}}}